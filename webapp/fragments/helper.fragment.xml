<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog
        id="_IDGenDialog2"
        title="Select Worksheet"
        draggable="true"
        resizable="true"
        contentWidth="300px"
    >
        <VBox
            id="_IDGenVBox2"
            class="sapUiSmallMargin"
        >
            <Label
                id="_IDGenLabel5"
                text="Worksheet"
            />
            <ComboBox
                id="_IDGenSelect1"
                items="{sheetModel>/sheets}"
                selectedKey="{sheetModel>/selectedSheet}"
                placeholder="Select a worksheet"
            >
                <core:Item
                    id="_IDGenItem"
                    key="{sheetModel>sheetName}"
                    text="{sheetModel>sheetName}"
                />
            </ComboBox>
        </VBox>
        <beginButton>
            <Button
                id="_IDGenButton5"
                text="OK"
                type="Emphasized"
                press="onSheetConfirm"
            />
        </beginButton>
        <endButton>
            <Button
                id="_IDGenButton4"
                text="Cancel"
                press="onSheetCancel"
            />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
