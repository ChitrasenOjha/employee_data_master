{
  "_version": "1.28.0",
  "sap.app": {
    "id": "employeedatamaster",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "3b6b7fab-534a-4996-b1fe-1efed72dc666"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZR_VALIDATION_SF_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "employeeCsfData": {
        "uri": "/sap/opu/odata/sap/ZR_VALIDATION_SF_CSF_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/employeeCsfData/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "employeeCompData": {
        "uri": "/sap/opu/odata/sap/ZR_VALIDATION_SF_COMP_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/employeeCompData/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.84.18",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "employeedatamaster.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "csfModel": {
        "dataSource": "employeeCsfData",
        "preload": false,
        "settings": {
          "useBatch": true
        }
      },
      "compModel": {
        "dataSource": "employeeCompData",
        "preload": false,
        "settings": {
          "useBatch": true
        }
      },
      "employeeCsfData": {
        "dataSource": "employeeCsfData",
        "preload": true,
        "settings": {}
      },
      "employeeCompData": {
        "dataSource": "employeeCompData",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "employeedatamaster.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMainPage",
          "pattern": ":?query:",
          "target": [
            "TargetMainPage"
          ]
        }
      ],
      "targets": {
        "TargetMainPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "MainPage",
          "viewName": "MainPage"
        }
      }
    },
    "rootView": {
      "viewName": "employeedatamaster.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
